{
  "name": "citgm",
  "version": "1.11.1",
  "description": "The Canary in the Goldmine",
  "homepage": "http://nodejs.org",
  "main": "bin/citgm",
  "preferGlobal": true,
  "bin": {
    "citgm": "./bin/citgm.js",
    "citgm-all": "./bin/citgm-all.js"
  },
  "scripts": {
    "test": "npm run lint && npm run tap",
    "tap": "tap -J --timeout 90 \"test/**/test-*.js\"",
    "coverage": "npm run tap -- --coverage",
    "coverage-html": "npm run tap -- --coverage --coverage-report=html",
    "lint": "eslint bin/* lib/ test/"
  },
  "author": "James M Snell <jasnell@gmail.com> (http://jasnell.me)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodejs/citgm"
  },
  "bugs": {
    "url": "https://github.com/nodejs/citgm/issues",
    "email": "jasnell@gmail.com"
  },
  "man": [
    "./man/citgm.1",
    "./man/citgm-all.1"
  ],
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.5",
    "chalk": "^1.1.3",
    "columnify": "^1.5.4",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "normalize-git-url": "^3.0.2",
    "npm-package-arg": "^4.2.0",
    "osenv": "^0.1.4",
    "read-package-json": "^2.0.4",
    "request": "^2.79.0",
    "rimraf": "^2.6.0",
    "root-check": "^1.0.0",
    "semver": "^5.3.0",
    "strip-ansi": "^3.0.1",
    "supports-color": "^3.2.3",
    "tar": "^2.2.1",
    "uid-number": "0.0.6",
    "update-notifier": "^2.1.0",
    "uuid": "^3.0.1",
    "which": "^1.2.12",
    "winston": "^2.3.1",
    "xml-sanitizer": "^1.1.6",
    "xmlbuilder": "^8.2.2",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "eslint": "^3.16.0",
    "ncp": "^2.0.0",
    "rewire": "^2.5.2",
    "string-to-stream": "^1.1.0",
    "tap": "^10.2.0",
    "tap-parser": "^5.3.3",
    "xml2js": "^0.4.17"
  }
