{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    no-trailing-spaces: 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
